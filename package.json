{
  "name": "jrgen",
  "version": "2.1.1",
  "description": "Generates docs, tests, clients and servers for json-rpc apis.",
  "keywords": [
    "generator",
    "json-rpc",
    "api",
    "doc",
    "test",
    "client",
    "server"
  ],
  "author": "Marco Zernetsch <m.zernetsch@gmail.com>",
  "license": "MIT",
  "bin": "./src/index.js",
  "bugs": "https://github.com/mzernetsch/jrgen/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzernetsch/jrgen.git"
  },
  "scripts": {
    "build:examples": "npm run clear:examples && node ./scripts/create-examples.js",
    "clear:examples": "node ./scripts/clear-examples.js",
    "format": "prettier --write './generators/**/!(templates)/*.{js,ts,css,scss,json,md}'"
  },
  "dependencies": {
    "commander": "^5.1.0",
    "deasync": "^0.1.20",
    "deepmerge": "^4.2.2",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "json-schema-ref-parser": "^9.0.1",
    "json-schema-to-typescript": "../json-schema-to-typescript",
    "jsonlint": "^1.6.3",
    "jsonschema": "^1.2.6",
    "markdown-table": "^2.0.0",
    "prettier": "^2.0.5",
    "uuid": "^8.1.0"
  }
}
